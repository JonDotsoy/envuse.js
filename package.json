{
  "name": "envuse",
  "version": "4.1.2-beta.0",
  "description": "Module to load environment variables from a `.envuse` file. Ideal to load configurations from the environment system, transform values and configure default values.",
  "publishConfig": {
    "access": "public",
    "tag": "beta"
  },
  "engines": {
    "node": ">=14"
  },
  "type": "module",
  "main": "./cjs/envuse.js",
  "types": "./esm/envuse.d.mts",
  "exports": {
    ".": {
      "import": "./esm/envuse.mjs",
      "require": "./cjs/envuse.cjs"
    }
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest watch",
    "prepare": "husky install && npm run build",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "build:esm": "tsc -p tsconfig.esm.json && echo '{}' > .def.json && echo 'export interface MapParsers {}' > storeTypeReference.d.ts"
  },
  "keywords": [],
  "author": "Jonathan Delgado <hi@jon.soy> (https://jon.soy)",
  "license": "MIT",
  "dependencies": {
    "@envuse/wasm": "^1.2.0"
  },
  "repository": {
    "url": "https://github.com/JonDotsoy/envuse.js.git",
    "type": "git"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^10.0.1",
    "@types/node": "^18.11.18",
    "husky": "^8.0.3",
    "prettier": "^2.8.1",
    "pretty-quick": "^3.1.3",
    "rollup": "^3.9.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vitest": "^0.26.2"
  }
}
